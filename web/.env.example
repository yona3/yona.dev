# Environment Variables for yona.dev
# Copy this file to .env.local and fill in the actual values

# Required: microCMS API key for blog content
# Get this from your microCMS dashboard
NEXT_PUBLIC_API_KEY=your-microcms-api-key-here

# Optional: Google Analytics tracking ID
# Format: GA_ID=G-XXXXXXXXXX
GA_ID=your-google-analytics-id

# Optional: Enable webpack bundle analyzer
# Set to "true" to generate bundle analysis report
ANALYZE=false